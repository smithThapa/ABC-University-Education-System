 {\r\n      //move top\r\n      window.scrollTo(0, 0);\r\n      showAlert(\r\n        'success',\r\n        'Your request to your new password is success!',\r\n        'An email has been send you to reset your password within 24 hours.'\r\n      );\r\n      //return login\r\n      window.setTimeout(() => {\r\n        location.assign('/');\r\n      }, 0);\r\n    }\r\n  } catch (err) {\r\n    // console.log(err.response.data);\r\n    window.scrollTo(0, 0);\r\n    showAlert('danger', 'Your request was decline!', err.response.data.message);\r\n  }\r\n};\r\n"},"lineCount":null}},"error":null,"hash":"70bafc89ecd97e33b694e924502a7ac0","cacheData":{"env":{}}}